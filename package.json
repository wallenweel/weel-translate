{
  "name": "weel-translate",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "wallen <wallen_weel@outlook.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run dev:watch",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "dev:ext": "web-ext run -s=dist --start-url about:debugging#addons -p ./web-ext-profile --keep-profile-changes",
    "dev:ext56": "web-ext run -s=dist --start-url about:debugging#addons -p ./web-ext-profile --keep-profile-changes --pre-install --firefox=C:/Applications/Firefox/App/Firefox/firefox.exe",
    "dev:watch": "npm run build:dev -- --watch",
    "dev:server": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --content-base dist/",
    "build:ext": "web-ext build -s=dist",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --config build/webpack.dev.conf.js",
    "build:pro": "node build/build.js",
    "release": "rimraf dist/ && npm run build:pro && web-ext build -s=dist"    
  },
  "dependencies": {
    "deepmerge": "^2.0.1",
    "js-md5": "^0.7.3",
